{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/search-songs",
      "dest": "/api/search-songs.ts"
    },
    {
      "src": "/api/add-song",
      "dest": "/api/add-song.ts"
    },
    {
      "src": "/api/create-map",
      "dest": "/api/create-map.ts"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/build/assets/$1"
    },
    {
      "src": "/(favicon\\.ico|favicon\\.svg|logo\\.svg|apple-touch-icon\\.png)",
      "dest": "/build/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/build/index.html"
    }
  ]
}
